{
  "name": "@9renpoto/archive",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/9renpoto/archive/issues"
  },
  "dependencies": {
    "@9renpoto/style": "^0.0.3",
    "fecha": "^2.3.2",
    "next": "^4.2.3",
    "nextein": "^1.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "^2.4.0",
    "@9renpoto/tslint-config": "^3.4.0",
    "@types/next": "^2.4.7",
    "@types/react": "^16.0.31",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "eslint": "^4.17.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "markdownlint-cli": "^0.7.1",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.0",
    "textlint": "^10.1.4",
    "textlint-rule-incremental-headers": "^0.2.0",
    "textlint-rule-ja-no-mixed-period": "^2.0.0",
    "textlint-rule-ja-no-successive-word": "^1.0.2",
    "textlint-rule-ja-no-weak-phrase": "^1.0.4",
    "textlint-rule-max-ten": "^2.0.3",
    "textlint-rule-no-dead-link": "^4.3.0",
    "textlint-rule-no-double-negative-ja": "^1.0.5",
    "textlint-rule-no-doubled-conjunction": "^1.0.2",
    "textlint-rule-no-doubled-conjunctive-particle-ga": "^1.0.2",
    "textlint-rule-no-doubled-joshi": "^3.5.1",
    "textlint-rule-no-dropping-the-ra": "^1.1.2",
    "textlint-rule-no-hankaku-kana": "^1.0.2",
    "textlint-rule-no-mix-dearu-desumasu": "^3.0.3",
    "textlint-rule-no-nfd": "^1.0.1",
    "textlint-rule-preset-ja-spacing": "^2.0.1",
    "textlint-rule-preset-jtf-style": "^2.3.1",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.0",
    "typescript": "^2.7.1",
    "typescript-eslint-parser": "^13.0.0"
  },
  "homepage": "https://github.com/9renpoto/archive#readme",
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "textlint --cache --fix",
      "markdownlint",
      "git add"
    ]
  },
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/archive.git"
  },
  "scripts": {
    "assets": "cpx node_modules/@9renpoto/style/lib/*.{css} static",
    "build": "nextein build",
    "clean": "rimraf out .next && find components/ pages/ utils/ -type f -name '*.js' -exec rm {} \\;",
    "export": "nextein export",
    "lint": "run-p lint:*",
    "lint:ja": "textlint posts --cache",
    "lint:md": "markdownlint posts",
    "lint:ts": "tslint {pages,component,post}/**/*.ts{,x}",
    "precommit": "lint-staged",
    "preexport": "run-s assets tsc build",
    "prestart": "run-s clean assets",
    "start": "concurrently \"tsc --pretty --watch\" \"nextein\"",
    "test": "npm run export",
    "tsc": "tsc"
  }
}
