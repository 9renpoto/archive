{
  "name": "@9renpoto/archive",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/9renpoto/archive/issues"
  },
  "dependencies": {
    "@9renpoto/style": "0.0.3",
    "fecha": "2.3.3",
    "next": "7.0.0",
    "nextein": "1.3.1",
    "prop-types": "15.6.2",
    "raven-js": "3.27.0",
    "react-dom": "16.5.2",
    "react-ga": "2.5.3"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.6.0",
    "@9renpoto/eslint-config-react": "4.6.0",
    "@9renpoto/textlint-config-ja": "4.6.0",
    "@9renpoto/tslint-config": "4.6.0",
    "@types/next": "7.0.0",
    "@types/react": "16.4.14",
    "@zeit/next-typescript": "1.1.1",
    "cpx": "1.5.0",
    "eslint": "5.6.1",
    "husky": "1.1.1",
    "lint-staged": "7.3.0",
    "markdownlint-cli": "0.13.0",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "size-limit": "0.20.1",
    "textlint": "11.0.0",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.1",
    "typescript-eslint-parser": "19.0.2"
  },
  "homepage": "https://github.com/9renpoto/archive#readme",
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "textlint --fix",
      "textlint",
      "markdownlint",
      "git add"
    ]
  },
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/archive.git"
  },
  "scripts": {
    "assets": "cpx -v -u 'node_modules/@9renpoto/style/lib/**/*.css' static",
    "build": "nextein build && size-limit",
    "clean": "rimraf out .next && find components/ pages/ utils/ -type f -name '*.jsx' -exec rm {} \\;",
    "export": "nextein export",
    "lint": "run-p lint:*",
    "lint:ja": "textlint posts --cache",
    "lint:md": "markdownlint posts",
    "lint:ts": "tslint {pages,component,post}/**/*.ts{,x}",
    "preexport": "run-p assets build",
    "prestart": "run-s clean assets",
    "start": "nextein",
    "test": "npm run export"
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/commons.*.js",
      "limit": "76 KB"
    },
    {
      "path": ".next/static/runtime/main-*.js",
      "limit": "5.5 KB"
    },
    {
      "path": ".next/static/runtime/webpack-*.js",
      "limit": "500 B"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
