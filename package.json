{
  "name": "@9renpoto/archive",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/9renpoto/archive/issues"
  },
  "dependencies": {
    "@9renpoto/style": "0.0.3",
    "fecha": "2.3.3",
    "next": "6.0.1",
    "nextein": "1.2.4",
    "prop-types": "15.6.1",
    "raven-js": "3.25.1",
    "react-dom": "16.3.2",
    "react-ga": "2.5.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.4.0",
    "@9renpoto/eslint-config-react": "4.3.0",
    "@9renpoto/textlint-config-ja": "4.4.0",
    "@9renpoto/tslint-config": "4.0.0",
    "@types/next": "2.4.10",
    "@types/react": "16.3.14",
    "@zeit/next-typescript": "0.1.1", "cpx": "1.5.0",
    "eslint": "4.19.1",
    "husky": "0.14.3",
    "lint-staged": "7.1.0",
    "markdownlint-cli": "0.8.2",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "size-limit": "0.18.0",
    "textlint": "10.2.1",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.8.3",
    "typescript-eslint-parser": "15.0.0"
  },
  "homepage": "https://github.com/9renpoto/archive#readme",
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "textlint --fix",
      "textlint",
      "markdownlint",
      "git add"
    ]
  },
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/archive.git"
  },
  "scripts": {
    "assets": "cpx -v -u 'node_modules/@9renpoto/style/lib/**/*.css' static",
    "build": "nextein build && size-limit",
    "clean": "rimraf out .next && find components/ pages/ utils/ -type f -name '*.jsx' -exec rm {} \\;",
    "export": "nextein export",
    "lint": "run-p lint:*",
    "lint:ja": "textlint posts --cache",
    "lint:md": "markdownlint posts",
    "lint:ts": "tslint {pages,component,post}/**/*.ts{,x}",
    "precommit": "lint-staged",
    "preexport": "run-p assets build",
    "prestart": "run-s clean assets",
    "start": "nextein",
    "test": "npm run export"
  },
  "size-limit": [
    {
      "path": ".next/static/commons/main-*.js",
      "limit": "90 KB"
    }
  ]
}
