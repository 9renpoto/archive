{
  "name": "@9renpoto/archive",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/9renpoto/archive/issues"
  },
  "dependencies": {
    "@9renpoto/style": "0.0.3",
    "fecha": "2.3.3",
    "next": "4.2.3",
    "nextein": "1.2.2",
    "prop-types": "15.6.1",
    "raven-js": "3.23.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-ga": "2.4.1"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.4.0",
    "@9renpoto/textlint-config-ja": "4.4.0",
    "@9renpoto/tslint-config": "4.0.0",
    "@types/next": "2.4.8",
    "@types/react": "16.0.40",
    "concurrently": "3.5.1",
    "cpx": "1.5.0",
    "eslint": "4.19.0",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "markdownlint-cli": "0.7.1",
    "npm-run-all": "4.1.2",
    "prettier-eslint-cli": "4.7.1",
    "textlint": "10.1.4",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "typescript-eslint-parser": "14.0.0",
    "webpack": "4.1.1"
  },
  "homepage": "https://github.com/9renpoto/archive#readme",
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "textlint --cache --fix",
      "markdownlint",
      "git add"
    ]
  },
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/archive.git"
  },
  "scripts": {
    "assets": "cpx -v -u 'node_modules/@9renpoto/style/lib/**/*.css' static",
    "build": "nextein build",
    "clean": "rimraf out .next && find components/ pages/ utils/ -type f -name '*.js' -exec rm {} \\;",
    "export": "nextein export",
    "lint": "run-p lint:*",
    "lint:ja": "textlint posts --cache",
    "lint:md": "markdownlint posts",
    "lint:ts": "tslint {pages,component,post}/**/*.ts{,x}",
    "precommit": "lint-staged",
    "preexport": "run-s assets tsc build",
    "prestart": "run-s clean assets",
    "start": "concurrently \"tsc --pretty --watch\" \"nextein\"",
    "test": "npm run export",
    "tsc": "tsc"
  }
}
