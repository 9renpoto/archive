{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "removeComments": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "jsx": "react-native",
    "module": "commonjs",
    "strict": true,
    "target": "es2017"
  }
}
